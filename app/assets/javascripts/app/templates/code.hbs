<div class="tabbable">
  <ul class="nav nav-tabs">
    <li {{bindAttr class="isModelView:active"}}>
      <a href="#" {{action setView "model"}}>Models</a>
    </li>
    <li {{bindAttr class="isRouterView:active"}}>
      <a href="#" {{action setView "router"}}>Router</a>
    </li>
    <li {{bindAttr class="isRouteView:active"}}>
      <a href="#" {{action setView "route"}}>Routes</a>
    </li>
    <li {{bindAttr class="isControllerView:active"}}>
      <a href="#" {{action setView "controller"}}>Controllers</a>
    </li>
    <li {{bindAttr class="isViewView:active"}}>
      <a href="#" {{action setView "view"}}>Views</a>
    </li>
    <li {{bindAttr class="isTemplateView:active"}}>
      <a href="#" {{action setView "template"}}>Templates</a>
    </li>
    <li {{bindAttr class="isTestView:active"}}>
      <a href="#" {{action setView "test"}}>Tests</a>
    </li>
  </ul>
  <div class="tab-content">

    {{#if isModelView}}
    <pre>
      // MODELS
      {{partial "code/ember_models"}}
    </pre>
    {{/if}}

    {{#if isRouterView}}
    <pre>
      {{partial "code/router"}}
    </pre>
    {{/if}}

    {{#if isRouteView}}
    <pre>
      {{partial "code/routes"}}
    </pre>
    {{/if}}
  </div>

</div>

