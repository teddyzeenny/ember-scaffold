<div class="row-fluid">
<div class="span6">
  <h2>Builder</h2>
  <div class="row-fluid .progress-tracker">
    {{#linkTo "custom_app.index" class="span2 progress-tracker-step" }}
      <div class="progress-tracker-step__bullet">1</div>
      <div class="progress-tracker-step__line"></div>
      <div class="progress-tracker-step__text">App Info</div>
    {{/linkTo}}
    {{#linkTo "ember_models" class="span2 progress-tracker-step" }}
      <div class="progress-tracker-step__bullet">2</div>
      <div class="progress-tracker-step__line"></div>
      <div class="progress-tracker-step__text">Define your models</div>
    {{/linkTo}}
    {{#linkTo "resources" class="span2 progress-tracker-step" }}
      <div class="progress-tracker-step__bullet">3</div>
      <div class="progress-tracker-step__line"></div>
      <div class="progress-tracker-step__text">Setup your routes</div>
    {{/linkTo}}

    {{#linkTo "preview" class="span2 progress-tracker-step"}}
      <div class="progress-tracker-step__bullet">4</div>
      <div class="progress-tracker-step__line"></div>
      <div class="progress-tracker-step__text">Preview App</div>
    {{/linkTo}}
    <div class="span2 progress-tracker-step">
      <div class="progress-tracker-step__bullet">4</div>
      <div class="progress-tracker-step__line"></div>
      <div class="progress-tracker-step__text">Download Kit</div>
    </div>
  </div>
  <div class="row-fluid">
    <div class="span12 builder">
      {{outlet}}
    </div>
  </div>
</div>
<div class="span6">
  <h2>Code</h2>
  {{outlet code}}
</div>